<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../styles/stylesAdmin.css">
    <title>Registro</title>
</head>
<body>
    <section class="cont__nav">
    
        <nav class="nav__administrador">
            <div class="cont__txt__admin">
                <p>Administrador</p>
            </div>
        </nav>
    </section>

    <section class="cont__form__agregarcard">
        <form action="/inmobiliaria/registro" method="POST">
            <div class="item-form">
                <label for="nombre">Coloque su nombre</label><br>
                <input type="text" name="nombre" id="nombre" class="form-control"><br>

                <label for="usuario">Cree usuario</label><br>
                <input type="text" name="usuario" id="usuario" class="form-control"><br>
                <label for="password">Cree contrase√±a</label><br>
                <input type="password" name="password" id="password" class="form-control"><br>

                <button class="boton-agregar">Crear Usuario</button>
            </div>
        </form>
    </section>

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <% if(typeof alert != "undefined"){ %>
        <script>

            Swal.fire({
                title:"<%= alertTitle %>",
                text:"<%= alertMessage %>",
                icon:"<%= alertIcon %>",
                showConfirmButton: "<%= showConfirmButton %>",
                timer: "<%= timer %>"
            }).then(()=>{
                window.location="/<%=ruta%>"
            })
        </script>
    <% } %>    
</body>
</html>